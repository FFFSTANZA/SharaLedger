{
	"compilerOptions": {
		"target": "ES2020",
		"module": "commonjs",
		"jsx": "react-jsx",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true",
		"declaration": true,
		"outDir": "./dist",
		"rootDir": ".",
		"moduleResolution": "node"
	},
	"include": ["src/**/*", "*.tsx"]
}